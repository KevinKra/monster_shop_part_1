<h4>Merchant Coupons Index</h4>

<% @coupons.each do |coupon| %>
  <article class="coupon-card" id="coupon-<%=coupon.id%>">
    <h4><%=coupon.name%></h4>
    <p>code: <%=coupon.code%></p>
    <p>active: <%=coupon.active%></p>
    <p>discount: <%=coupon.discount%></p>
    <%= link_to 'delete', "merchant/coupons/:id" %>
  </article>
<% end %>