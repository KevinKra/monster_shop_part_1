
  <%= form_tag "/users/profile", method: :patch do %>
    <%= label_tag :name %>
    <%= text_field_tag :name%>

    <%= label_tag :street_address %>
    <%= text_field_tag :street_address %>

    <%= label_tag :city %>
    <%= text_field_tag :city%>

    <%= label_tag :state %>
    <%= text_field_tag :state %>

    <%= label_tag :zip %>
    <%= text_field_tag :zip%>

    <%= label_tag :email %>
    <%= text_field_tag :email %>

    <%= submit_tag 'Update' %>
  <% end %>
