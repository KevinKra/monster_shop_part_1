<h2>Hello,<%=@user.name%>!</h2>
<%= @user.street_address %>
<%= @user.city %>
<%= @user.state %>
<%= @user.zip %>
<%= @user.email %>


<section class="user-dashboard">
  <h2>User Profile</h2>
  <%= link_to "My Orders", "/profile/orders" if @user.has_orders? %>
	<%= button_to 'Edit Password', "/profile/edit_password", method: :get %>
  <%= link_to "Edit Information", "/users/#{current_user.id}/edit" %>
</section>
